wb = xlsx_package.workbook
wb.add_worksheet(name: 'Biodiversity results') do |ws|
	@csv_string.split('\n').each do |to_row|
	  	ws.add_row to_row.split(',')
	end
end

wb.add_worksheet(name: 'Enzyme commission numbers') do |ws|
	@tsv_ec_string.split('\n').each do |to_row|
	  	ws.add_row to_row.split('\t')
	end
end
