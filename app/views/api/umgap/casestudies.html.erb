      <div class="card-actions card-border">
        <a href='<%= api_clidocs_casestudy_metagenomics_path %>' class='btn-text pull-right'>get started</a>
      </div>
    </div>
  </div>
  <div class="col-xs-3">
    <%= render 'api/shared/sidebar' %>
  </div>
</div>
