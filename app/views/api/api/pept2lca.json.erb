<%=
  Oj.dump(
    @input_order.map do |k|
      seq_index = @equate_il ? k.gsub(/I/,'L') : k
      p = @result[seq_index]
      res = { "sequence" => k,
              "taxon_id" => p.id,
              "taxon_name" => p.name,
              "taxon_rank" => p.rank,
      }
      if @extra_info
        res.merge!(lineage_info(p.lineage, @names))
      else
        res
      end
  end).html_safe
%>
